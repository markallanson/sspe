<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configuration>
    <system.diagnostics>
      <trace autoflush="false" indentsize="4">
        <listeners>
          <add name="fileLogger"
               type="System.Diagnostics.TextWriterTraceListener"              
               initializeData="LogFile.log" />
          <remove name="Default" />
        </listeners>
      </trace>
    </system.diagnostics>
  </configuration>
</configuration>